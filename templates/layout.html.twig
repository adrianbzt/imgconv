<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <link rel="icon" type="image/x-icon" href="{{ asset('build/favicon.ico') }}" />

    <title>{% block title %}Image Conversion!{% endblock %}</title>
    {% block stylesheets %}
        <!-- Global stylesheets -->
        <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">

        <link rel="stylesheet" href="{{ asset('build/app.css') }}">

        <!-- /global stylesheets -->

    {% endblock %}
</head>
<body>
{% block body %}
    {% include "header.html.twig" %}

    {% include 'main.html.twig' %}

    {% include "footer.html.twig" %}
{% endblock %}

{% block javascripts %}

    <script src="{{ asset('build/app.js') }}"></script>
    {#<script src="{{ asset('build/jquery.js') }}"></script>#}
    <script src="{{ asset('build/bootstrap.js') }}"></script>
    <script src="{{ asset('build/importTweaks.js') }}"></script>
{% endblock %}
</body>
</html>
